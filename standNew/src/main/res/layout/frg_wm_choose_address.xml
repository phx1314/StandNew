<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">
     <LinearLayout android:layout_width="match_parent" android:layout_height="@dimen/j44dp" android:gravity="center_vertical" android:orientation="horizontal" android:paddingBottom="@dimen/j7dp" android:paddingTop="@dimen/j7dp">
          <LinearLayout android:id="@+id/mLinearLayout_back" android:layout_width="@dimen/j45dp" android:layout_height="match_parent" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:gravity="center" android:padding="@dimen/j8dp" android:paddingLeft="@dimen/j10dp">
               <ImageButton android:id="@+id/btn_left" style="@style/text_style_15_white" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="#00000000" android:gravity="center" android:src="@drawable/arrows_left" />
          </LinearLayout>
          <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginRight="@dimen/j10dp" android:background="@drawable/yslt_bj_hyss_n" android:gravity="center_vertical" android:orientation="horizontal">
               <EditText android:id="@+id/mEditText_search" style="@style/text_style_13_black" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1" android:background="#00000000" android:hint="请输入地址" android:padding="@dimen/j5dp" android:singleLine="true" />
               <ImageButton android:id="@+id/mImageButton_serach" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/j5dp" android:background="#00000000" android:src="@drawable/yslt_bt_search_n" />
          </LinearLayout>
     </LinearLayout>
     <RelativeLayout android:layout_width="fill_parent" android:layout_height="@dimen/j200dp">
          <com.amap.api.maps.MapView android:id="@+id/map" android:layout_width="match_parent" android:layout_height="@dimen/j200dp" />
          <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true" android:background="@drawable/bg_lan" />
     </RelativeLayout>
     <ImageView android:layout_width="fill_parent" android:layout_height="1px" android:background="@color/tyD1" />
     <ListView android:id="@+id/lv_address" android:layout_width="fill_parent" android:layout_height="fill_parent" android:divider="@drawable/line" android:listSelector="#00000000" android:scrollbars="none" />
</LinearLayout>

